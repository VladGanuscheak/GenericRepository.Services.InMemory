<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <PackageId>GenericRepository.Services.InMemory</PackageId>
    <Version>2.1.0</Version>
    <Authors>Ganu»ôceac Vlad</Authors>
    <Product>GenericRepository.Services.InMemory</Product>
    <Title>GenericRepository.Services.InMemory</Title>
    <Description>Implements InMemory version of the GenericRepository's contracts.</Description>
    <RepositoryUrl>https://github.com/VladGanuscheak/GenericRepository.Services.InMemory</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>generic;repository;genericrepository;.net;dotnet;c#;contracts;services;inmemory</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageReleaseNotes>Added DI. Migrated to .NetCore 2.0</PackageReleaseNotes>
    <PackageIcon>logo.png</PackageIcon>
    <PackageIconUrl />
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="C:\Users\User\Desktop\Logos\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
    
  <ItemGroup>
    <PackageReference Include="GenericRepository.Contracts" Version="2.1.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="7.0.0" />
  </ItemGroup>
    
  <ItemGroup>
    <PackageReference Update="Microsoft.NETCore.App" Version="2.1.30" />
  </ItemGroup>

</Project>
